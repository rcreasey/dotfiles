pool {
  name: "DEV Environment Meta"
  name_replace: "DEV$1-ENVIRONMENT"
  findFilter: "^DEV(7\\d)-ALL"
  findFilter: "^DEV(7\\d)-GAMESERVICE"
  findFilter: "^dev(7\\d)-hardware"
  findFilter: "^DEV(7\\d)-OLYMPUS"
}

pool {
  name: "DEV All Service Meta"
  name_replace: "DEV$1-ALL-SERVICES"
  findFilter: "^DEV(7\\d)-\\w+(?:-\\d+)?-[^\\d\\-]*"
}

pool {
  name: "DEV T1 Copy Gameservice Meta"
  name_replace: "DEV$1-AURORA"
  findFilter: "^DEV(2|3|6|10|11|2[1-5]|3\\d|4\\d|5\\d|6\\d|7\\d)-BN-(?:BACK|FRONT|OBERON|MEMCACHED|MINION|ODDJOB|PORT|RISKAUTH|RISKMETER|SOCIAL|WARDEN)"
  findFilter: "^DEV(2|3|6|10|11|2[1-5]|3\\d|4\\d|5\\d|6\\d|7\\d)-D3-(?:MATCHBATCH|MATCHCOOP)"
  findFilter: "^DEV(2|3|6|10|11|2[1-5]|3\\d|4\\d|5\\d|6\\d|7\\d)-WTCG-(?:MATCHBATCH|MATCHCOOP)"
  findFilter: "^DEV(2|3|6|10|11|2[1-5]|3\\d|4\\d|5\\d|6\\d|7\\d)-HAD-(?:MATCHBATCH|MATCHCOOP)"
  findFilter: "^DEV(2|3|6|10|11|2[1-5]|3\\d|4\\d|5\\d|6\\d|7\\d)-DB-(?:AURORA)"
}

pool {
  name: "DEV T1 Copy BGSOlympus Meta"
  name_replace: "DEV$1-BGSOLYMPUS"
  findFilter: "^DEV(6|10|11|3\\d|4\\d|5\\d|6\\d|7\\d)-BN-(?:DEFILER|ROMEO|GATEWAY|SUNKEN)"
  findFilter: "^DEV(6|10|11|3\\d|4\\d|5\\d|6\\d|7\\d)-CORE-(?:SCOURGE|STARPORT)"
}

pool {
  name: "DEV Olympus Meta"
  name_replace: "DEV$1-OLYMPUS"
  findFilter: "^DEV(2|3|6|10|11|2[1-5]|3\\d|4\\d|5\\d|6\\d|7\\d)-(?:CORE|HERO|S2|T1)-.*"
  findFilter: "^DEV(2|3|6|10|11|2[1-5]|3\\d|4\\d|5\\d|6\\d|7\\d)-DB-(?:GSPIRE|CEREBRATE|KALOTH)(?:\\d+)?"
  findFilter: "^DEV(2|3|6|10|11|2[1-5]|3\\d|4\\d|5\\d|6\\d|7\\d)-BN-(?:DEFILER|SCOURGE|SUNKEN|GATEWAY|STARPORT|ROMEO)\\d+"
}

