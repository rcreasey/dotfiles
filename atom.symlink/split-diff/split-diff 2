  findFilter: "^XX-BN-(\\d+)-(?:DEFILER|SCOURGE|SUNKEN|GATEWAY|STARPORT|ROMEO)\\d+"
  findFilter: "^(?:USX|EUX|KRX|AMSX|AUXX|CNX|SGX)-HERO-(\\d+)-GAME"
  findFilter: "^(?:USX|EUX|KRX|AMSX|AUXX|CNX|SGX)-S2-(\\d+)-GAME"
}

