pool {
  name: "OLYMPUS-ALL"
  findFilter: "^OLYMPUS-DEV$"
  findFilter: "^OLYMPUS-STAGING$"
  findFilter: "^OLYMPUS-PTR$"
  findFilter: "^OLYMPUS-PROD$"
}

pool {
  name: "OLYMPUS-DEV"
  findFilter: "^DEV\\d*-OLYMPUS$"
  findFilter: "^BN\\d*-OLYMPUS$"
}

pool {
  name: "OLYMPUS-STAGING"
  findFilter: "^ST\\d*-OLYMPUS$"
}

pool {
  name: "OLYMPUS-PTR"
  findFilter: "^XX-\\d*-OLYMPUS$"
}

pool {
  name: "OLYMPUS-PROD"
  findFilter: "^(US|EU|SG|KR|CN)-OLYMPUS-MAINTENANCE$"
  findFilter: "^(US|EU|SG|KR|CN)-HEROES-MAINTENANCE$"
  findFilter: "^(US|EU|SG|KR|CN)-STARCRAFT2-MAINTENANCE$"
  findFilter: "^(US|EU|SG|KR|CN)-BN-DEFILER$"
  findFilter: "^(US|EU|SG|KR|CN)\\d*-DATABASES$"
}
